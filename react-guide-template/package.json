{
  "name": "react-guide-template",
  "description": "react guide template",
  "version": "0.0.1",
  "author": "allanyu(allanyu@tencent.com)",
  "license": "MIT",
  "keywords": [
    "react",
    "qqbrowser",
    "ui-framework"
  ],
  "engines": {
    "node": ">4.4.0"
  },
  "scripts": {
    "links": "npm link node-sass fsevents",
    "build": "cross-env NODE_ENV=development better-npm-run build && npm run copy-lib",
    "dev": "cross-env NODE_ENV=development node webpack/bin/startup.js",
    "prod": "cross-env NODE_ENV=production node webpack/bin/startup.js",
    "build-dev": "better-npm-run build-dev && npm run copy-lib",
    "copy-lib": "mkdir -p dist/js && cp -fr webpack/lib/* dist/js/",
    "lint": "eslint -c .eslintrc src api",
    "dll": "webpack --config webpack/webpack.dll.js --display-chunks --profile  --colors --display-error-details && npm run copy-lib"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --progress --display-chunks --profile  --colors --display-error-details  --config webpack/webpack.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-dev": {
      "command": "webpack --progress --display-chunks --profile  --colors --display-error-details --config webpack/webpack.dev.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "@tencent/react-guide": "*",
    "better-npm-run": "0.0.8",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.3",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "core-decorators": "^0.12.3",
    "history": "=1.17.0",
    "js-beautify": "^1.6.4",
    "lodash": "^3.10.1",
    "morgan": "^1.7.0",
    "piping": "^1.0.0-rc.3",
    "react": "~15.3.1",
    "react-dom": "~15.3.1",
    "react-helmet": "^3.1.0",
    "request": "^2.74.0",
    "scroll-behavior": "^0.3.2",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "webpack-isomorphic-tools": "^2.5.7"
  },
  "repository": {
    "url": "http://git.oa.com/mxd/react-guide",
    "type": "git"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-module-resolver": "^2.1.1",
    "babel-plugin-react-intl": "2.1.3",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "clean-webpack-plugin": "^0.1.6",
    "concurrently": "^2.2.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "memory-fs": "^0.3.0",
    "node-sass": "3.8.0",
    "postcss-cssnext": "^2.5.2",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.3.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "script-loader": "^0.7.0",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "uuid": "^2.0.2",
    "webpack": "^2.1.0-beta.6",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
